#!/usr/bin/env node

require('babel/register')({
  only: /(sagui\/commands)|(sagui\/src)/
})


var base = require('../commands/base');
var develop = require('../commands/develop');
var test = require('../commands/test');


var command = process.argv[2];
var parameters = process.argv.slice(3);


base.run(parameters);


switch (command) {
  case 'develop':
    develop.run(parameters);
    break;
  case 'test':
    test.run(parameters);
    break;
}
